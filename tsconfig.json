{
  "compilerOptions": {
    "jsx": "react",
    "paths": {
      "~*": ["src/**/*"]
    },
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "target": "es5",
    "strict": true,
    "noEmit": true,
    "noErrorTruncation": true,
    "allowJs": true,
    "checkJs": true
  }
}
